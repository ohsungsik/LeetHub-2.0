{
  "name": "LeetHub v2",
  "version": "2.0.6",
  "private": true,
  "description": "Automatically integrate your code with LeetCode and GitHub",
  "scripts": {
    "dev": "webpack --mode=production --watch",
    "setup": "npm i",
    "format": "prettier --write **/*.{js,jsx,ts,tsx,css,html}",
    "format-test": "prettier --check **/*.{js,jsx,ts,tsx,css,html}",
    "lint": "eslint **/*.{js,jsx,ts,tsx} --fix",
    "lint-test": "eslint **/*.{js,jsx,ts,tsx} --color",
    "build": "webpack --mode=production"
  },
  "devDependencies": {
    "chrome-types": "^0.1.288",
    "copy-webpack-plugin": "^12.0.2",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "filemanager-webpack-plugin": "^8.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "puppeteer": "^22.12.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "rimraf": "^4.4.1",
    "glob": "^9.3.5",
    "core-js-pure": "^3.23.3"
  }
}
